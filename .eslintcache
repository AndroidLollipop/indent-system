.log(tagcollection.length/idf[iks[i]])\n    }\n    else{\n      idf[iks[i]] = 0\n    }\n  }\n}\nvar makeRanker = (data) => {\n  var run = 0\n  var idf = {}\n  var tf_ = []\n  return (terms) => {\n    var terms = terms.trim().toLowerCase().split(/\\b\\s+/)\n    if (!run){\n      computeTfIdfs(data, tf_, idf)\n      run = 1\n    }\n    var ret = []\n    for (var i = 0; i < data.length; i++){\n      ret.push([0, 0, i, data[i]])\n    }\n    for (var i = 0; i < terms.length; i++){\n      var term = terms[i]\n      for (var j = 0; j < data.length; j++){\n        if (tf_[j][term]){\n          ret[j][0] -= tf_[j][term] * idf[term]\n          ret[j][1] -= tf_[j][term]\n        }\n      }\n    }\n    ret = ret.sort((a, b) => {\n      var i = 0\n      while(a[i] === b[i]){\n        i++\n      }\n      return a[i]-b[i]\n    }).map(x => x[3])\n    return ret\n  }\n}\nmodule.exports = {\n  makeRanker: makeRanker\n}",{"ruleId":"35","severity":1,"message":"36","line":111,"column":7,"nodeType":"37","endLine":111,"endColumn":42},{"ruleId":"38","severity":1,"message":"39","line":116,"column":7,"nodeType":"40","messageId":"41","endLine":116,"endColumn":15},{"ruleId":"38","severity":1,"message":"42","line":141,"column":10,"nodeType":"40","messageId":"41","endLine":141,"endColumn":14},{"ruleId":"43","severity":1,"message":"44","line":267,"column":34,"nodeType":"45","messageId":"46","endLine":272,"endColumn":6},{"ruleId":"38","severity":1,"message":"47","line":284,"column":23,"nodeType":"40","messageId":"41","endLine":284,"endColumn":30},{"ruleId":"38","severity":1,"message":"48","line":284,"column":32,"nodeType":"40","messageId":"41","endLine":284,"endColumn":43},{"ruleId":"38","severity":1,"message":"49","line":284,"column":56,"nodeType":"40","messageId":"41","endLine":284,"endColumn":68},{"ruleId":"35","severity":1,"message":"36","line":640,"column":165,"nodeType":"37","endLine":640,"endColumn":212},{"ruleId":"38","severity":1,"message":"50","line":666,"column":7,"nodeType":"40","messa